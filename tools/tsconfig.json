{
    "compilerOptions": {
      "esModuleInterop": true,
      "lib": [
          "es2015",
          "es5",
          "dom"
        ],
      "module": "commonjs",
      "target": "es2015",
      "moduleResolution": "node",
  
      "outDir": "./lib/",
  
      "declaration": true,
      "declarationDir": "./types",
  
      "preserveSymlinks": true,
  
      "preserveWatchOutput": true,
      "pretty": false,
  
      "forceConsistentCasingInFileNames": true,
  
      "noFallthroughCasesInSwitch": true,
      "noImplicitAny": true,
      "noImplicitReturns": true,
      "noUnusedLocals": true
    },
    "include": [
        "./src/*.ts",
        "./src/commands/*.ts"
    ],
    "exclude": [ ]
  }
  